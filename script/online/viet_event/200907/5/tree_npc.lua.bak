	if GetName() ~= strsub(npcName, 1, nNameLen) then --ÓÃÃû×ÖÀ´ÅÐ¶ÏÊÇ²»ÊÇÍæ¼ÒµÄÊ÷
		Msg2Player("§©y kh«ng ph¶i lµ c©y b¹n trång");
